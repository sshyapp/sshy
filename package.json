{
    "name": "sshy",
    "productName": "sshy",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": ".webpack/main",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "tslint -c tslint.json -p tsconfig.json",
        "assets-dev": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "assets-prod": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "dev": "yarn run assets-dev && yarn run start"
    },
    "keywords": [],
    "author": {
        "name": "Levente Otta",
        "email": "leventeotta@gmail.com"
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "sshy"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                [
                    "@electron-forge/plugin-webpack",
                    {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./dist/app.js",
                                    "name": "main_window"
                                }
                            ]
                        }
                    }
                ]
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "6.0.0-beta.47",
        "@electron-forge/maker-deb": "6.0.0-beta.47",
        "@electron-forge/maker-rpm": "6.0.0-beta.47",
        "@electron-forge/maker-squirrel": "6.0.0-beta.47",
        "@electron-forge/maker-zip": "6.0.0-beta.47",
        "@electron-forge/plugin-webpack": "6.0.0-beta.47",
        "@grafite/helpers": "^1.0.0",
        "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
        "bootstrap": "^4.4.1",
        "cross-env": "^7.0.0",
        "css-loader": "^3.0.0",
        "electron": "8.0.1",
        "fork-ts-checker-webpack-plugin": "^3.1.1",
        "jquery": "^3.4.1",
        "laravel-mix": "^5.0.1",
        "lodash": "^4.17.15",
        "node-loader": "^0.6.0",
        "popper.js": "^1.16.1",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.25.0",
        "sass-loader": "^8.0.2",
        "style-loader": "^0.23.1",
        "ts-loader": "^6.2.1",
        "tslint": "^5.20.0",
        "typescript": "^3.7.0",
        "vue": "^2.5.17",
        "vue-loader": "^15.9.0",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "electron-squirrel-startup": "^1.0.0"
    }
}
